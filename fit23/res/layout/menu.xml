<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent">

	<com.markupartist.android.widget.ActionBar
		android:id="@+id/actionbar" style="@style/ActionBar" />

	<ScrollView android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_weight="1">
		<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			xmlns:app="http://schemas.android.com/apk/res/fit.main"
			android:orientation="vertical" android:layout_width="fill_parent"
			android:layout_height="fill_parent">
			
			
						<TableLayout android:stretchColumns="*"
				android:layout_width="fill_parent" android:id="@+id/tableLayout1"
				android:gravity="top" android:paddingTop="4dip"
				android:layout_height="fill_parent" android:layout_alignParentTop="true"
				android:layout_alignParentLeft="true">

				<TableRow android:layout_height="wrap_content" android:id="@+id/tableRow1"
					android:layout_width="wrap_content" android:paddingBottom="1dip">

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_conta_pessoal" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/conta_pessoal"
							android:id="@+id/imageButton1" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Conta Pessoal"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView1" android:layout_width="wrap_content" />
					</LinearLayout>

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_notificacoes" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/notificacoes"
							android:id="@+id/imageButton2" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Notificações"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView2" android:layout_width="wrap_content" />
					</LinearLayout>

				</TableRow>

				<TableRow android:layout_height="wrap_content" android:id="@+id/tableRow2"
					android:layout_width="wrap_content" android:paddingBottom="1dip">

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_plano_treino" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/plano_treino"
							android:id="@+id/imageButton3" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Plano de Treino"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView3" android:layout_width="wrap_content" />
					</LinearLayout>

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_plano_alimentar" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/plano_alimentar"
							android:id="@+id/imageButton4" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Plano Alimentar"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView4" android:layout_width="wrap_content" />
					</LinearLayout>

				</TableRow>

				<TableRow android:layout_height="wrap_content" android:id="@+id/tableRow3"
					android:layout_width="wrap_content" android:paddingTop="1dip">

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_aulas" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/aulas" android:id="@+id/imageButton5"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:background="@null" android:layout_gravity="center_horizontal" />
						<TextView android:text="Aulas" android:layout_gravity="center_horizontal"
							android:layout_height="wrap_content" android:id="@+id/textView5"
							android:layout_width="wrap_content" />
					</LinearLayout>

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_ginasio_mais_proximo"
						android:layout_height="wrap_content" android:orientation="vertical"
						android:paddingBottom="6dip" android:paddingTop="6dip"
						android:paddingLeft="10dip" android:paddingRight="10dip">
						<ImageView android:src="@drawable/ginasio_mais_proximo"
							android:id="@+id/imageButton6" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Ginásio + Próximo"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView6" android:layout_width="wrap_content" />
					</LinearLayout>

				</TableRow>

				<TableRow android:layout_height="wrap_content" android:id="@+id/tableRow4"
					android:layout_width="wrap_content" android:paddingBottom="1dip">

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_pagamentos" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/pagamentos"
							android:id="@+id/imageButton7" android:layout_width="wrap_content"
							android:layout_height="wrap_content" android:background="@null"
							android:layout_gravity="center_horizontal" />
						<TextView android:text="Pagamentos"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView7" android:layout_width="wrap_content" />
					</LinearLayout>

					<LinearLayout android:layout_width="wrap_content"
						android:id="@+id/layout_contactos" android:layout_height="wrap_content"
						android:orientation="vertical" android:paddingBottom="6dip"
						android:paddingTop="6dip" android:paddingLeft="10dip"
						android:paddingRight="10dip">
						<ImageView android:src="@drawable/contactos" android:id="@+id/imageButton8"
							android:layout_width="wrap_content" android:layout_height="wrap_content"
							android:background="@null" android:layout_gravity="center_horizontal" />
						<TextView android:text="Contactos"
							android:layout_gravity="center_horizontal" android:layout_height="wrap_content"
							android:id="@+id/textView8" android:layout_width="wrap_content" />
					</LinearLayout>

				</TableRow>

			</TableLayout>
		</LinearLayout>
	</ScrollView>

</LinearLayout>
